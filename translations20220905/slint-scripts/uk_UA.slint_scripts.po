msgstr "\"$program_name -l\" - Список доступних кодів мов"

# File: spd-list, line: 16
#, sh-format
msgid "\"$program_name -ls <synthesizer>\" languages available for this synthesizer?"
msgstr "\"$program_name -ls <назва синтезатора>\" - Список мов, що підтримує вказаний синтезатор мовлення"

# File: spd-list, line: 17
#, sh-format
